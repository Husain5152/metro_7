<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_reserved_list_wizard" model="ir.ui.view">
            <field name="name">view_set_pr_line_supplier_wizard</field>
            <field name="model">reserved.list.wizard</field>
            <field name="arch" type="xml">
                <form string="Reserved PR" version="7.0">
                    <separator string="List of Reserved PR for this product"/>
                    <group>
                        <field name="product_id"/>
                    </group>
                    <field name="pr_reserved_ids" colspan="4" nolabel="1"/>
                    <footer>
                        <button string="OK" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</openerp>